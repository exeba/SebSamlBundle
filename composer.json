{
    "name": "seb/saml-bundle",
    "description": "Symfony SebSamlBundle",
    "keywords": [
        "authentication",
        "sso",
        "saml"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiano Degan",
            "email": "sebdeg87@gmail.com"
        }
    ],
    "require": {
        "php": ">7.1",
        "seb/authenticator-bundle": ">=3.1.1",
        "onelogin/php-saml" : ">=3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seb\\SamlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seb\\SamlBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/test-pack": "*"
    }
}
